<div class="banner-show" style="background-image: linear-gradient(rgba(0,0,0,0.6), rgba(0,0,0,0.3)), url(https://res.cloudinary.com/dqi7etmsi/image/upload/v1560255826/adult-african-afro-1083619_ngo8gi.jpg);">

 <div class="container-retour" data-aos="fade-up">
  <%= link_to projects_path do %>
  <h3 id="section-retour"><i class="far fa-arrow-alt-circle-left"></i>Retour aux missions</h3>
  <% end %>
</div>

  <div class="container-dashboard">
    <div class="row" id="dashboard-elements">
      <h4 class="bookings-title">Mes inscriptions</h4>
    </div>

    <div class="cards" id="cardsbooking">
      <% @bookings.each do |booking| %>
          <div class="card" data-aos="fade-up" data-booking-id='<%= booking.id %>'>
            <div class="card-img-project" style="background-image: url(<%= booking.mission.project.photo %>)">
            </div>
            <div id="yellow">
              <h5 class="project-name"><strong><%= booking.mission.project.charity.name%></strong></h5>
                <h3 class="charity-name"><%= booking.mission.project.name%></h3>
              </div>
            <div class="card-body" id="cardtextbooking">
                  <p><i class="fas fa-calendar-week"></i>  <strong><%= booking.mission.start_time.strftime("%m/%d/%Y %k") %>h</strong> au: <strong> <%= booking.mission.end_time.strftime("%m/%d/%Y %k") %>h</strong></p>
                  <p><i class="fas fa-map-marker-alt"></i>  <%= booking.mission.project.address%></p>
                  <!-- <img src="https://res.cloudinary.com/patti01/image/upload/v1559763641/govo/cancel_iodrwh.png" alt="cancel" width=32px> -->
              <strong>
                <%= link_to booking_path(booking),
                method: :delete, remote: true, class: 'delete_booking', data: { confirm: 'Êtes-vous sûr.e ?' } do %>
                  <i class="far fa-times-circle"></i>
                  Annuler
                <% end %>

              </strong>
            </div>
          </div>
      <%end%>
    </div>
  </div>
  <div style="height: 50px;"></div>
</div>


    <meta property="og:title" content="Bound." />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="<%= request.original_url %>" />
    <meta property="og:image" content="https://i.ibb.co/jwCRhZx/bound-og.jpg" />
    <meta property="og:description" content="<%= meta_description %>" />
    <meta property="og:site_name" content="Bound." />

