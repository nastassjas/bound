<div class="banner-show" style="background: linear-gradient(45deg, rgba(18,64,55,1) 0%, rgba(20,76,82,1) 76%, rgba(20,76,82,1) 100%);">

  <div class="container-dashboard">
    <div class="dashboard-elements">
      <div class="row">
        <div class="col-lg-6">
          <h4>Mes inscriptions</h4>
        </div>
        <div class="col-lg-6">
          <h4>Mon profil</h4>
        </div>
      </div>
    </div>
    <div class="dashboard-mybookings">
      <div class="cards-dashboard">
        <% @missions.each do |mission| %>
        <div class="card-img-txt">
            <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.6), rgba(0,0,0,0.3)), url(https://images.unsplash.com/photo-1469571486292-0ba58a3f068b?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60)">
              <p style="font-size: 20px; text-align: center"><%= mission.project.name%></p>
            </div>
            <div class="card-txt">
            <p><%= mission.project.charity.name%></p>
            <p><i class="fas fa-map-marker-alt"></i>  <%= mission.project.address%></p>
            <p><%= mission.start_time.strftime("%m/%d/%Y %k") %>h - <%= mission.end_time.strftime("%m/%d/%Y %k") %>h</p>
            <img src="https://res.cloudinary.com/patti01/image/upload/v1559763641/govo/cancel_iodrwh.png" alt="cancel" width=32px>
            </div>

          </div>
          <% end %>
      </div>

    </div>
    <div class="dashboard-myprofile">
    </div>
  </div>
</div>
